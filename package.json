{
  "name": "multimodal-login",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --mode development --config webpack.config.js",
    "webpack:watch": "webpack --mode development --config webpack.config.js --watch",
    "start": "npx babel-node src/app.js",
    "dev": "npx nodemon --ignore 'src/static/' --exec 'npm run webpack &&  COMPILER_ENV=server babel-node src/app.js'",
    "test": "npx jest --config=jest.config.js",
    "prebuild": "rm -rf dist/*",
    "build": "npm run webpack-production && npm run babel-production",
    "postbuild": "cp -r src/static/* dist/static",
    "webpack-production": "webpack -p",
    "babel-production": "NODE_ENV=production COMPILER_ENV=server npx babel --out-dir dist src",
    "build:start": "npm run build && node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cityofaustin/multimodal-login.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cityofaustin/multimodal-login/issues"
  },
  "homepage": "https://github.com/cityofaustin/multimodal-login#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.10.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "express-oauth-server": "^2.0.0",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.18",
    "node-fetch": "^2.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.1",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.8.3",
    "@svgr/webpack": "^5.4.0",
    "@types/jest": "^26.0.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
